{
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 3000
    },
    "singleProjectMode": true
  },
  "hosting": {
    "public": "app/build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "minecraft-server-automation",
          "appId": "1:526285433869:android:69b9a5621335c3ce513944",
          "fileOutput": "app/android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "minecraft-server-automation",
          "appId": "1:526285433869:ios:e9118f2900663962513944",
          "uploadDebugSymbols": false,
          "fileOutput": "app/ios/Runner/GoogleService-Info.plist"
        }
      },
      "macos": {
        "default": {
          "projectId": "minecraft-server-automation",
          "appId": "1:526285433869:ios:e9118f2900663962513944",
          "uploadDebugSymbols": false,
          "fileOutput": "app/macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "app/lib/firebase_options.dart": {
          "projectId": "minecraft-server-automation",
          "configurations": {
            "android": "1:526285433869:android:69b9a5621335c3ce513944",
            "ios": "1:526285433869:ios:e9118f2900663962513944",
            "macos": "1:526285433869:ios:e9118f2900663962513944",
            "web": "1:526285433869:web:a34b3f0d55c55314513944",
            "windows": "1:526285433869:web:46ccdb68f69189e2513944"
          }
        }
      }
    }
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
  "extensions": {}
}